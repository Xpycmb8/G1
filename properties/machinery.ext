)

(define "movement_wheel"
	{Chassis "track"
		{Locomotion 
			{Slow   %slow }   
			{Normal %normal }
			{Fast   %fast }
			{maxspeed %maxspeed}
			{maxSpeedAtMaxTurn %maxspeed_atmaxturn}; max speed at max turn, km/h
    		{TurnStart 		%turnstart} 
    		{TurnStop		%turnstop}; time to go from maximum turn speed to zero, seconds
			{TurnRadius		%turnradius}; minimum turn radius for AI, meters
			{TurnTime		%turntime}	; 360 degree turn time at max turn speed, seconds		
			{StartTime		%starttime}		; acceleration time from 0 to max speed, seconds	
    		{StopTime		%stoptime} 	; stop time after turning off the engine (no breaking), seconds		
    		{BrakeTime		%braketime} ; forced deceleration time from max speed to 0, seconds
			{Gears		0.1 0.3 0.5 0.7 1.0}	; forward gear ratios
			{RearGears	0.2 0.5}				; backward gear ratios
			{GearSwitchDelay 1}				; time to switch from forward gear to rear one, sec
			
			{MaxSlope 	240}																							
			{FallSlope 	240}			
		}
		{FuelBag
			{volume %fuelbag_vol}						
			{rate %rate}
			{remain %remain}						
			{fuel "%fuel"}						
		}		
		{ignorePatherBounds}
		{disableBackward}
	}
)
	
	
