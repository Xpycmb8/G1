(define "movement_skirmisher"
	{Chassis "human_carrier"
		{Locomotion 
			{Slow   			0 }   
			{Normal 			9 } ;+
			{Fast   			45 }
			{maxspeed 			45}
			{maxSpeedAtMaxTurn 5}
    		{TurnStart 		10} 
    		{TurnStop		0.5}
			{TurnRadius		5}
			{TurnTime		1.5}			
			{StartTime		5}			
    		{StopTime		5} 			
    		{BrakeTime		2.5} 

			{Gears 0.1 1	}														;// Forward gear ratios
			{RearGears 0.05 				}					;// Reverse max speed.
			{GearSwitchDelay 3			}																;// Time to switch between forward and reverse.
			
			{MaxSlope 	240}																							
			{FallSlope 	240}			
		}
		{FuelBag
			{volume 200}						
			{rate 0.5}
			{remain 190}						
			{fuel "tritium"}						
		}		
		{ignorePatherBounds}
		{disableBackward}
	}
)		
