		{if stuff "damage_overlap"
			{tear "piece_small_wood" bone from_volume
				{impulse up 0.5 0.2  	dir 8 4		cx 4 2 cy 4 2  	no_position}
			}
		else
			(define "blast_hit"
				{if min_energy %blast
					{if rand %shard
						{start_sound "crush/medium/wood/"}
