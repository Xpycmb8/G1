{"wooden_walls"
	{inflammation_time 10}
	{on spawn
		{link_sound 	"start_small" 			"fire/start_small/emt_fire_med_fuel_01"}
		{link_sound 	"flame_small"			"fire/small_stuff/fire_small"}		
	}
	{on "burn_volume"
		{call "fire_sound"}
		{delay 40
			{stop_sound "flame_small" }	
		}
		;{if not "burned"
			;{set "burned" 1}
		(define "cut_subpiece_wood"
			{if volume "wood_%0"
				{if not tagged "burned_wood_%0"
					{tags add "burned_wood_%0"}
					{burn volume "wood_%0" fx "smoke_wood_small" time 20 fake even_law}	
					{delay 10 3
						{burn volume "wood_%0" fx "smoke_wood_small_fire" time 90 fake even_law}	
						{fire volume "wood_%0" radius 0.1 heating 2  time 60 follow}
						{delay 80 20							
							{tear "piece_small_wood_burn" bone "wood_plank_down_%0"
								{volumes disable contact }	
								{tex_morph "burned" 10}
								{fire radius 0.1 heating 2  time 15 follow}
								{burn volume "wood_%0" fx "smoke_wood_small_fire" time 20 fake even_law}	
								{impulse forward 0.1}
								{start_sound "fire/crush_wood/"}
							}
						}
					}
				}
			}
		)
		("cut_subpieces_wood")
		;}
	}
	{on "fire_sound"
		{if not tagged "burned_sound"
			{tags add "burned_sound"}
			;{play_sound "start_small"}
			{play_sound "flame_small" 1}	
			{delay 20
				{tags remove "burned_sound"}
			}
		}
	}
	{on pierce
		(define "bullet_hit"
			{if stuff %stuff
				{if rand %rand
					{if volume "wood"
						{start_sound "crush/medium/wood/"}
						{tear "piece_medium_wood_crushed" bone from_volume
							{tags add "crush"}
							;{burn volume fx "tree_fall" time 0.5 fake even_law}
							%impact
						}
					}
				}
			}
		)
		;                  stuff   		   
		("bullet_hit" stuff("shell")			rand(1) 		impact({impulse dir 7 4		cx 4 2 cy 4 2  	no_position}))
		("bullet_hit" stuff("heavy_zenite")		rand(0.6) 		impact({impulse dir 8 4 	cx 7 3 cy 7 3  	no_position}))
		("bullet_hit" stuff("zenite")			rand(0.3) 		impact({impulse dir 4 2 	cx 4 1 cy 4 1 	no_position}))
		("bullet_hit" stuff("bullet big")		rand(0.3) 		impact({impulse dir 2 1 	cx 2 1 cy 2 1 	no_position}))
		("bullet_hit" stuff("bullet normal")	rand(0.1) 		impact({impulse dir 1 0.5 	cx 0.5 cy 0.5  	no_position}))
		("bullet_hit" stuff("bullet small")		rand(0.01) 		impact({impulse dir 0.5 	cx 0.5 cy 0.5  	no_position}))
	}
	{on contact 		
		{if effector "car"
			{if contact_velocity 25
				(define "cut_subpiece_wood"
					{if rand 0.01
						{tear "piece_small_wood" bone "wood_plank_down_%0"
							{impulse forward 0.5 0.2 cx 2 1 cy 2 1 }
						}
					}
				)
				("cut_subpieces_wood")
			}
		}
