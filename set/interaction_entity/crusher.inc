			(define "contact_hit"
				{if effector %0
					{if contact_velocity 10
						{if volume "wood"
							{if rand %1
								{start_sound "crush/medium/wood/"}
								{tear "piece_small_wood" bone from_volume
									{impulse dir 1.5 0.7 cx 2 1 cy 2 1 rnd_dir 2 no_position}
								}
							}
						}
					}
				}
			)
			;                  ------     
			("contact_hit" args "fraction"	0.2 )
			("contact_hit" args "vehicle"	1 )
